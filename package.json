{
	"publisher": "Leason",
	"name": "vsrecoder",
	"displayName": "vsrecoder",
	"description": "recoder for vscode",
	"icon": "images/icon.png",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"record"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.vsrecoder.showRecode",
				"title": "Show VSRecode"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.vsrecoder.showRecode",
					"group": "commandPalette"
				}
			]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/leasontou/vsrecoder.git"
	},
	"license": "MIT"
}
